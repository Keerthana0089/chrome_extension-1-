{
    "manifest_version": 2,
    "name": "Tab Title Picker",
    "version": "1.0",
    "description": "A Chrome extension that picks up the title of the current tab",
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}